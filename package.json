  },
  "devDependencies": {
    "@prisma/client": "4.3.1",
    "dotenv-cli": "^6.0.0",
    "eslint": "8.23.0",
    "eslint-config-next": "12.3.0",
    "prisma": "^4.3.1",
    "jsdoc": "^3.6.10",
